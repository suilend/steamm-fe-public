{
  "name": "@suilend/steamm-sdk-local",
  "version": "0.0.21",
  "private": false,
  "description": "A TypeScript SDK for interacting with the STEAMM program",
  "author": "Suilend",
  "license": "MIT",
  "main": "./sdk/src/index.ts",
  "exports": {
    ".": "./sdk/src/index.js"
  },
  "types": "./sdk/src/index.ts",
  "scripts": {
    "build": "rm -rf ./dist && bun tsc",
    "eslint": "eslint --fix \"./src/**/*.ts\"",
    "prettier": "prettier --write \"./src/**/*\"",
    "lint": "bun eslint && bun prettier && bun tsc --noEmit",
    "test": "bun test",
    "release": "bun run build && bun ts-node ./release.ts && cd ./dist && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suilend/steamm-fe.git"
  },
  "bugs": {
    "url": "https://github.com/suilend/steamm-fe/issues"
  },
  "dependencies": {
    "@pythnetwork/pyth-sui-js": "^2.1.0",
    "@suilend/frontend-sui": "^0.2.58",
    "@suilend/sdk": "^1.1.52",
    "bignumber.js": "^9.1.2",
    "dotenv": "^16.4.7",
    "p-limit": "^6.2.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.17",
    "@types/chai": "^4.3.19",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "chai": "^5.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "@mysten/move-bytecode-template": "0.1.0"
  },
  "peerDependencies": {
    "@mysten/sui": "1.28.2"
  }
}
